{
    "name": "rah/textpattern-project-template",
    "description": "Textpattern CMS project template",
    "type": "project",
    "keywords": [
        "textpattern"
    ],
    "homepage": "https://github.com/gocom/textpattern-project-template",
    "license": "MIT",
    "authors": [
        {
            "name": "Jukka Svahn",
            "homepage": "https://github.com/gocom",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/gocom/textpattern-project-template/issues",
        "source": "https://github.com/gocom/textpattern-project-template"
    },
    "require": {
        "php": "^8.1",
        "textpattern/lock": "4.8.8",
        "rah/rah_flat": "^0.4.3",
        "rah/rah_sitemap": "^4.0",
        "rah/rah_autoload": "^0.3.2",
        "rah/rah_memcached": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "private": true,
    "config": {
        "allow-plugins": {
            "textpattern/installer": true
        }
    }
}
